{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "api/test.js", "use": "@vercel/node" },
    { "src": "api/simple.js", "use": "@vercel/node" },
    { "src": "api/minimal.js", "use": "@vercel/node" },
    { "src": "api/debug.js", "use": "@vercel/node" },
    { "src": "api/users.js", "use": "@vercel/node" },
    { "src": "api/products.js", "use": "@vercel/node" },
    { "src": "api/init.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/init", "dest": "/api/init.js" },
    { "src": "/api/products", "dest": "/api/products.js" },
    { "src": "/api/products/(.*)", "dest": "/api/products.js" },
    { "src": "/api/users", "dest": "/api/users.js" },
    { "src": "/api/users/(.*)", "dest": "/api/users.js" },
    { "src": "/api/debug", "dest": "/api/debug.js" },
    { "src": "/api/minimal", "dest": "/api/minimal.js" },
    { "src": "/api/simple", "dest": "/api/simple.js" },
    { "src": "/api/test", "dest": "/api/test.js" },
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
} 